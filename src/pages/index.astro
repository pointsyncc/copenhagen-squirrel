---
import Layout from "../layouts/Layout.astro";
import Card from "../components/Card.astro";
import { MobileHeader } from "../components/ui/Navigation/MobileHeader.tsx";
import HeroSectionBanner from "../components/ui/Banners/HeroSectionBanner.astro";
import { DesktopHeader } from "../components/ui/Navigation/DesktopHeader.tsx";
---

<Layout title="Početna">
  <main>
 <!--    <MobileHeader client:load />  -->
    <!-- Hero Section -->
    <section class="hero_section mt-2.5 md:mt-0">
      <HeroSectionBanner />
      <div
